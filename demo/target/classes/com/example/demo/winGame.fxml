<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.willhero.WinGameController">
   <ImageView fx:id="background" fitHeight="768.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@assets/background.png" />
   </ImageView>
   <Group layoutX="10.0" layoutY="10.0">
      <ImageView fx:id="btnMusic" fitHeight="55.0" fitWidth="55.0" onMouseClicked="#onMusicButtonClick" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnmusic0.png" />
      </ImageView>
      <ImageView fx:id="btnSound" fitHeight="55.0" fitWidth="55.0" layoutX="73.0" onMouseClicked="#onSoundButtonClick" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnsound0.png" />
      </ImageView>
      <ImageView fitHeight="55.0" fitWidth="55.0" layoutX="146.0" onMouseClicked="#onBackButtonClick" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnback.png" />
      </ImageView>
   </Group>
   <ImageView fx:id="light" fitHeight="200.0" fitWidth="200.0" layoutX="419.0" layoutY="240.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@assets/light.png" />
   </ImageView>
   <ImageView fx:id="hero" fitHeight="49.0" fitWidth="65.0" layoutX="505.0" layoutY="310.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@assets/hero.png" />
   </ImageView>
   <Text fill="#0fab1c" layoutX="360.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You Won!!">
      <font>
         <Font size="72.0" />
      </font>
   </Text>
</AnchorPane>
